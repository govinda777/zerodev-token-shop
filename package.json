{
  "name": "zerodev-token-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest --testPathPattern=\"usePrivyAuth|LoginDemo|PrivyLoadingScreen|ProductCard|AuthButton|MockAuthProvider|TokenProvider\" --passWithNoTests --watchAll=false",
    "test:unit:watch": "jest --testPathPattern=\"usePrivyAuth|LoginDemo|PrivyLoadingScreen|ProductCard|AuthButton|MockAuthProvider|TokenProvider\" --watch",
    "test:unit:coverage": "jest --testPathPattern=\"usePrivyAuth|LoginDemo|PrivyLoadingScreen|ProductCard|AuthButton|MockAuthProvider|TokenProvider\" --coverage --passWithNoTests --watchAll=false",
    "test:all": "jest --passWithNoTests --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:critical": "playwright test -g @critical",
    "test:e2e:no-critical": "playwright test -g @no-critical",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:all": "node scripts/run-tests.js",
    "test:e2e:critical-only": "node scripts/run-tests.js --tag @critical",
    "push:skip-tests": "git push --no-verify",
    "push:all-tests": "node scripts/pre-push-test.js --all-tests && git push",
    "prepare": "husky",
    "compile": "hardhat compile",
    "deploy:sepolia": "hardhat run scripts/deploy.ts --network sepolia",
    "deploy:local": "hardhat run scripts/deploy.ts --network localhost",
    "verify:sepolia": "hardhat run scripts/verify.ts --network sepolia",
    "test:claim": "hardhat run scripts/test-claim.ts --network sepolia",
    "test:claim:local": "hardhat run scripts/test-claim.ts --network localhost",
    "node:local": "hardhat node",
    "test:contracts": "hardhat test test/ZeroDevToken.test.ts test/TokenFaucet.test.ts",
    "blockchain:setup": "npm run compile && npm run deploy:sepolia && npm run verify:sepolia",
    "blockchain:test": "npm run test:claim",
    "clean": "rm -rf .next node_modules/.cache && npm install",
    "fix-deps": "npm update @privy-io/react-auth @zerodev/sdk @zerodev/ecdsa-validator && npm run clean"
  },
  "dependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@privy-io/react-auth": "^2.13.3",
    "@types/winston": "^2.4.4",
    "@zerodev/ecdsa-validator": "^5.4.9",
    "@zerodev/sdk": "^5.4.35",
    "next": "^15.3.3",
    "react": "^18",
    "react-dom": "^18",
    "uuid": "^9.0.1",
    "viem": "^2.29.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@account-abstraction/contracts": "^0.8.0",
    "@account-abstraction/sdk": "^0.6.0",
    "@babel/preset-typescript": "^7.27.1",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@openzeppelin/hardhat-upgrades": "^3.9.0",
    "@playwright/test": "^1.52.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "@typescript-eslint/parser": "^8.33.0",
    "autoprefixer": "^10.0.1",
    "dotenv": "^16.5.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "hardhat": "^2.17.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
